{
    "version": "0.2.0",
    "configurations": [
        
        
        {
            "type": "node",
            "request": "launch",
            "name": "Launch BasicSetupTest",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "internalConsole",
            "preLaunchTask": "cleanAndBuild",
            "outputCapture": "std",
            "autoAttachChildProcesses": false,
            "program": "${workspaceFolder}/src/Tests/BasicSetupTest/Server/TestIndex.ts",
            "internalConsoleOptions": "openOnSessionStart",
            "outFiles": [
                "${workspaceFolder}/build/serverTsBuild/**/*.js"
            ]
        }
    ]
}